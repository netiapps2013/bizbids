namespace BBids\BBidsHomeBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

class  Vendorenquiryrel{

    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $enquiryid;

    /**
     * @var integer
     */
    private $vendorid;


    /**
     * @var integer
     */
    private $acceptstatus;

/**
     * Get acceptstatus
     *
     * @return integer 
     */
    public function getAcceptstatus()
    {
        return $this->acceptstatus;
    }



    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set enquiryid
     *
     * @param integer $enquiryid
     * @return Vendorenquiryrel
     */
    public function setEnquiryid($enquiryid)
    {
        $this->enquiryid = $enquiryid;

        return $this;
    }

    /**
     * Get enquiryid
     *
     * @return integer 
     */
    public function getEnquiryid()
    {
        return $this->enquiryid;
    }

/**
     * Set acceptstatus
     *
     * @param integer $acceptstatus
     * @return Vendorenquirel
     */
    public function setAcceptstatus($acceptstatus)
    {
        $this->acceptstatus = $acceptstatus;

        return $this;
    }


    /**
     * Set vendorid
     *
     * @param integer $vendorid
     * @return Vendorenquiryrel
     */
    public function setVendorid($vendorid)
    {
        $this->vendorid = $vendorid;

        return $this;
    }

    /**
     * Get vendorid
     *
     * @return integer 
     */
    public function getVendorid()
    {
        return $this->vendorid;
    }
    /**
     * @var integer
     */
    private $leadstatus;


    /**
     * Set leadstatus
     *
     * @param integer $leadstatus
     * @return Vendorenquiryrel
     */
    public function setLeadstatus($leadstatus)
    {
        $this->leadstatus = $leadstatus;

        return $this;
    }

    /**
     * Get leadstatus
     *
     * @return integer 
     */
    public function getLeadstatus()
    {
        return $this->leadstatus;
    }
}
